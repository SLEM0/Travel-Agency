<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="OOP.Pages.Profile.Client.ClientChangePassword"
             Title="Изменить пароль">
    <VerticalStackLayout>
        <Label Text="Старый пароль" />
        <Entry x:Name="Old"/>
        <Label Text="Новый пароль" />
        <Entry x:Name="New"/>
        <Button Text="Ок" Clicked="Ok_Button_Clicked" BackgroundColor="#E85021" TextColor="White"/>
    </VerticalStackLayout>
</ContentPage>